<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="002" id="org.pebbleparkkids.mobile" version="0.02" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>pebble park kids</name>
    <description>Teach your kids about Internet Safety real time, each time they run in to a blocked page. Blocking alone is not enough!</description>
    <author email="contact@pebbleparkkids.org" href="http://pebbleparkkids.org">pebbleparkkids</author>
    <content src="index.html" />
    <access origin="*" />
    <access origin="content:///*" />
    <allow-navigation href="*" />
    <preference name="phonegap-version" value="cli-6.3.0" />
    <preference name="permissions" value="none" />
    <preference name="orientation" value="default" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="false" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="android-targetSdkVersion" value="23" />
    <preference name="android-installLocation" value="auto" />
    <preference name="disable-cursor" value="false" />
    <preference name="prerendered-icon" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarStyle" value="default" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="2000" />
    <gap:platform name="ios" />
    <gap:platform name="android" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="www/res/android/icon/drawable-ldpi-icon.png" />
        <icon density="mdpi" src="www/res/android/icon/drawable-mdpi-icon.png" />
        <icon density="hdpi" src="www/res/android/icon/drawable-hdpi-icon.png" />
        <icon density="xhdpi" src="www/res/android/icon/drawable-xhdpi-icon.png" />
        <icon density="xxhdpi" src="www/res/android/icon/drawable-xxhdpi-icon.png" />
        <icon density="xxxhdpi" src="www/res/android/icon/drawable-xxxhdpi-icon.png" />
        <splash density="land-ldpi" src="www/res/android/splash/drawable-land-ldpi-screen.png" />
        <splash density="land-mdpi" src="www/res/android/splash/drawable-land-mdpi-screen.png" />
        <splash density="land-hdpi" src="www/res/android/splash/drawable-land-hdpi-screen.png" />
        <splash density="land-xhdpi" src="www/res/android/splash/drawable-land-xhdpi-screen.png" />
        <splash density="land-xxhdpi" src="www/res/android/splash/drawable-land-xxhdpi-screen.png" />
        <splash density="land-xxxhdpi" src="www/res/android/splash/drawable-land-xxxhdpi-screen.png" />
        <splash density="port-ldpi" src="www/res/android/splash/drawable-port-ldpi-screen.png" />
        <splash density="port-mdpi" src="www/res/android/splash/drawable-port-mdpi-screen.png" />
        <splash density="port-hdpi" src="www/res/android/splash/drawable-port-hdpi-screen.png" />
        <splash density="port-xhdpi" src="www/res/android/splash/drawable-port-xhdpi-screen.png" />
        <splash density="port-xxhdpi" src="www/res/android/splash/drawable-port-xxhdpi-screen.png" />
        <splash density="port-xxxhdpi" src="www/res/android/splash/drawable-port-xxxhdpi-screen.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="57" src="www/res/ios/icon/icon.png" width="57" />
        <icon height="114" src="www/res/ios/icon/icon@2x.png" width="114" />
        <icon height="40" src="www/res/ios/icon/icon-40.png" width="40" />
